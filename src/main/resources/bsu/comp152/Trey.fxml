<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bsu.comp152.TreyController">
    <padding><Insets bottom="40" left="40" right="40" top="40" /></padding>
    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" maxWidth="Infinity" minWidth="100.0" prefWidth="100.0">
        </ColumnConstraints>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="200.0" prefWidth="200.0">
        </ColumnConstraints>
      <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="183.0" prefWidth="207.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="185.0" prefWidth="193.0" />
    </columnConstraints>


    <Label text="Jokes" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowSpan="1">
        <font>
            <Font name="Arial" size="24" />
        </font>
        <GridPane.margin>
            <Insets bottom="20" top="20" />
        </GridPane.margin>
    </Label>


    <Label text="Category : " GridPane.rowIndex="2">
    </Label>


   <Button fx:id="Any" mnemonicParsing="false" onAction="#anyJoke" text="Any" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   <Button fx:id="Programming" mnemonicParsing="false" onAction="#progJoke" text="Programming" GridPane.columnIndex="2" GridPane.rowIndex="2" />
   <Button fx:id="Dad" mnemonicParsing="false" onAction="#dadJoke" text="Dad" GridPane.columnIndex="3" GridPane.rowIndex="2" />

   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
